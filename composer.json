{
    "name": "zorachka/event-dispatcher",
    "description": "Event Dispatcher",
    "keywords": [
        "zorachka",
        "event dispatcher",
        "psr-14"
    ],
    "homepage": "https://github.com/zorachka/event-dispatcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/event-dispatcher": "^1.0",
        "webmozart/assert": "^1.10",
        "zorachka/container": "^2.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "pestphp/pest": "^1.21",
        "pestphp/pest-plugin-mock": "^1.0",
        "roave/security-advisories": "dev-latest"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\Framework\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
