{
    "name": "zorachka/event-dispatcher",
    "description": "Event Dispatcher",
    "keywords": [
        "zorachka",
        "event dispatcher",
        "psr-14"
    ],
    "homepage": "https://github.com/zorachka/event-dispatcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/event-dispatcher": "^1.0",
        "webmozart/assert": "^1.11",
        "zorachka/container": "^3.0"
    },
    "require-dev": {
        "dg/bypass-finals": "^1.3",
        "friendsofphp/php-cs-fixer": "^3.11",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-phpunit": "^0.17.0",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.28"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\EventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\EventDispatcher\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "php-cs-fixer": "php-cs-fixer --config=.php-cs-fixer.php --allow-risky=yes",
        "psalm": "psalm --config=psalm.xml",
        "test": "phpunit --colors=always"
    }
}
