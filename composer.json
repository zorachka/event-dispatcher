{
    "name": "zorachka/event-dispatcher",
    "description": "Event Dispatcher",
    "keywords": [
        "zorachka",
        "event-dispatcher"
    ],
    "homepage": "https://github.com/zorachka/event-dispatcher",
    "license": "MIT",
    "authors": [
        {
            "name": "Siarhei Bautrukevich",
            "email": "hello@bautrukevich.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "psr/event-dispatcher": "^1.0",
        "webmozart/assert": "^1.10"
    },
    "require-dev": {
        "brainmaestro/composer-git-hooks": "^2.8",
        "dg/bypass-finals": "^1.3",
        "friendsofphp/php-cs-fixer": "^2.17",
        "phpunit/phpunit": "^9.5",
        "roave/security-advisories": "dev-latest",
        "vimeo/psalm": "^4.3"
    },
    "autoload": {
        "psr-4": {
            "Zorachka\\EventDispatcher\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zorachka\\EventDispatcher\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "hooks": {
            "pre-commit": [
                "echo committing as $(git config user.name)",
                "make format"
            ],
            "pre-push": [
                "make format",
                "make test"
            ]
        }
    }
}
